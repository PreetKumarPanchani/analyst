{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/utils/*": ["src/utils/*"],
      "@/services/*": ["src/services/*"],
      "@/styles/*": ["src/styles/*"],
      "@/pages/*": ["src/pages/*"],
      "@/theme/*": ["src/theme/*"],
      "@/layouts/*": ["src/layouts/*"],

    },

  },
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["node_modules", ".next"],  
}